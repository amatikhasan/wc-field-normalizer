{
    "name": "wcfn/wc-field-normalizer",
    "description": "WooCommerce Field Normalizer",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "WCFN\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WCFN\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
